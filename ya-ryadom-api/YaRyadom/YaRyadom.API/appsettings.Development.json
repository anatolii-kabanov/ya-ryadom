		"RealIpHeader": "X-Real-IP",
		"ClientIdHeader": "X-ClientId",
		"HttpStatusCode": 429,
		"GeneralRules": [
			{
				"Endpoint": "*",
				"Period": "1s",
				"Limit": 3
			},
			{
				"Endpoint": "*",
				"Period": "1m",
				"Limit": 20
			},
			{
				"Endpoint": "*",
				"Period": "1h",
				"Limit": 900
			}
		]
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	}
