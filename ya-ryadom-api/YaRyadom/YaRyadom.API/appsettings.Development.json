{
	"AppSettings": {
		"SecretKey": "4uEksWtK4aWARJfuHlrP" // Put your app secret key here for Debug
	},
	"IpRateLimiting": {
		"EnableEndpointRateLimiting": true, // Limit splitted to different types of http (get, post e.t.c)
